pkgname=sdl_net
realname=SDL_net
pkgver=1.2.7
pkgrel=2
pkgdesc="A small sample cross-platform networking library"
url="http://www.libsdl.org/projects/SDL_net/"
depends=('sdl>=1.2.13-2' 'glibc')
groups=('xlib-extra')
archs=('i686' 'x86_64' 'ppc')
up2date="lynx -dump $url|grep -m1 'SDL_net-\(.*\).tar.gz'|sed 's/.*-\(.*\).t.*/\1/'"
source=($url/release/$realname-$pkgver.tar.gz)
_F_cd_path="$realname-$pkgver"
sha1sums=('b46c7e3221621cc34fec1238f1b5f0ce8972274d')

# optimization OK
