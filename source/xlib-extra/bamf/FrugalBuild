pkgname=bamf
pkgver=0.2.114
pkgrel=1
pkgdesc="BAMF Application Matching Framework."
depends=('dbus-glib' 'libwnck' 'libwnck2' 'libgtop' 'libuuid')
groups=('xlib-extra')
archs=('i686' 'x86_64')
_F_launchpad_branch="0.2"
_F_launchpad_subdir="0.2.114"
_F_gnome_glib="y"
Finclude launchpad gnome-scriptlet
sha1sums=('068e8caa0e304e39c909e395c6a00db55501a5e8')

build() {
    Fbuild --with-gtk=3
    make clean || Fdie
    Fbuild --with-gtk=2
    Fbuild_gnome_scriptlet
}

# optimization OK
