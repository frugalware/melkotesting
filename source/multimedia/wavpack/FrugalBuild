pkgname=wavpack
pkgver=4.60.1
pkgrel=1
pkgdesc="A completely open audio compression format providing lossless, high-quality lossy, and a unique hybrid compression mode."
url="http://www.wavpack.com/"
depends=()
groups=('multimedia')
archs=('i686' 'x86_64')
up2date="lynx -dump $url/downloads.html|grep -m1 'tar.bz2'|sed 's/.*-\(.*\).tar.bz2/\1/'"
source=($url/$pkgname-$pkgver.tar.bz2)
sha1sums=('003c65cb4e29c55011cf8e7b10d69120df5e7f30')

# optimization OK
