pkgname=gl2ps
pkgver=1.3.6
pkgrel=1
pkgdesc="An OpenGL to PostScript printing library"
url="http://geuz.org/gl2ps"
depends=()
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date="Flasttar $url"
source=($url/src/$pkgname-$pkgver.tgz)
Finclude cmake
_F_cd_path="$pkgname-$pkgver-source"
sha1sums=('ac36a0a82ceba0fb037b8357b01cd2383065fc2d')
