pkgname=racc
pkgver=1.4.5
pkgrel=1
pkgdesc="A LALR(1) parser generator for Ruby"
url="http://www.loveruby.net/en/racc.html"
depends=('ruby')
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump lynx -dump  $url |grep tar.gz |  grep $pkgname- | cut -d '-' -f 2"
_F_cd_path="$pkgname-$pkgver-all"
source=(http://i.loveruby.net/archive/racc/$_F_cd_path.tar.gz)
sha1sums=('80ec24d4c9db09812514b5b2f0db988ce7de2898')

# optimization OK
