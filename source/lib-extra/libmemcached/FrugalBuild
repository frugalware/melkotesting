pkgname=libmemcached
pkgver=1.0.6
pkgrel=1
pkgdesc="A C and C++ client library for memcached"
depends=('memcached')
groups=('lib-extra')
archs=('i686' 'x86_64')
_F_launchpad_branch="1.0"
Finclude launchpad
sha1sums=('54a031e95035a2b01c61aeef222f4380921719e2')

build() {
	Fcd
	Fconf --prefix=/usr
	Fmake
	Fmakeinstall
}

# optimization OK
