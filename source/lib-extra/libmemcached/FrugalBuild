pkgname=libmemcached
pkgver=1.0.8
pkgrel=1
pkgdesc="A C and C++ client library for memcached"
depends=('memcached')
groups=('lib-extra')
archs=('i686' 'x86_64')
_F_launchpad_branch="1.0"
Finclude launchpad
sha1sums=('ac1925aea002d8ad28d709aa374d057c73e322af')

build() {
	Fcd
	Fconf --prefix=/usr
	Fmake
	Fmakeinstall
}

# optimization OK
