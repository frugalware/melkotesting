pkgname=libexosip2
_F_archive_name="libeXosip2"
pkgver=4.0.0
pkgrel=1
pkgdesc="A library that hides the complexity of using SIP for multimedia session establishement"
url="http://savannah.nongnu.org/projects/exosip"
depends=('libosip2>=4.0.0' 'openssl')
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://download.savannah.gnu.org/releases/exosip | Flasttar"
source=(http://download.savannah.nongnu.org/releases/exosip/${_F_archive_name}-${pkgver/_/-}.tar.gz)
signatures=($source.sig)
