pkgver=2.3
pkgrel=2
pkgdesc="Text progressbar library for python"
depends=('python')
makedepends=('python-distribute')
groups=('lib-extra')
archs=('i686' 'x86_64')
_F_googlecode_name="progressbar"
_F_googlecode_dirname=$pkgname
Finclude googlecode
up2date="lynx -dump http://code.google.com/p/python-progressbar/downloads/list | sed 's/-dev//g' | Flasttar"
