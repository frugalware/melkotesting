pkgname=resdup
pkgver=0.0.4
pkgrel=1
pkgdesc="A commandline tool to remove duplicated lines from a file, with many sort options."
depends=('perl')
groups=('apps-extra')
archs=('i686' 'x86_64')
_F_googlecode_dirname="trizen"
Finclude googlecode
sha1sums=('d286f4a4db464f112656531bea7184575b6fb370')

build() {
	Fexe $pkgname /usr/bin/$pkgname
}
