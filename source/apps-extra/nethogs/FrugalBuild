pkgname=nethogs
pkgver=0.8.0
pkgrel=1
pkgdesc="NetHogs is a small 'net top' tool."
depends=('libpcap')
_F_cd_path="$pkgname"
Finclude sourceforge
groups=('apps-extra')
archs=('i686' 'x86_64')
sha1sums=('5c06a99efd2ac1d3e155ac63334f8d69589122ab')

build()
{
	Fsed 'sbin' 'bin' Makefile
	Fbuild
}

# optimization OK
