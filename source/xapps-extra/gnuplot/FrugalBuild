groups=('xapps-extra')
archs=('i686' 'x86_64')
Finclude sourceforge
url="http://www.gnuplot.info/"
Fconfopts="$Fconfopts --with-x --with-readline=gnu --with-linux-vga --disable-wxwidgets --without-lua"
