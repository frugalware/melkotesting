pkgname=lonote
pkgver=1.6.10
pkgrel=1
pkgdesc="LoNote is a personal note-taking software."
depends=('pyqt4')
makedepends=('python-3.0' '7zip')
groups=('xapps-extra')
archs=('i686' 'x86_64')
_F_googlecode_ext=".7z"
Finclude googlecode
sha1sums=('49480e5bfe7f2833add5424d9b9b172f6e5d27c2')

build() {
	Fcd
	python3 setup.py install --root=$Fdestdir || Fdie
}
