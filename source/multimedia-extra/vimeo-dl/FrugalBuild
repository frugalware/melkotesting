pkgname=vimeo-dl
pkgver=0.0.6
pkgrel=1
pkgdesc="A command line tool to download videos from vimeo.com"
depends=('perl-libwww')
groups=('multimedia-extra')
archs=('i686' 'x86_64')
_F_googlecode_dirname="trizen"
Finclude googlecode
sha1sums=('492fba126d58a45b49b51602c67d7cd8ed7c8907')

build() {
	Fexe $pkgname /usr/bin/$pkgname
}
