pkgrel=3
pkgdesc="A fully tested, abstract interface to creating OAuth clients and servers."
depends=('python>=2.7')
makedepends=('distribute')
groups=('devel-extra')
archs=('i686' 'x86_64')
_F_archive_name="oauth2"
_F_pypi_name='oauth2'
Finclude pypi python
