# Compiling Time: 0.01 SBU
# Maintainer: Miklos Vajna <vmiklos@frugalware.org>

pkgname=docutils
pkgver=0.10
pkgrel=1
pkgdesc="Documentation Utilities"
Finclude sourceforge
depends=('distribute')
groups=('devel-extra')
archs=('i686' 'x86_64')
sha1sums=('c2290465e17276348c7242a7d79dccb0af90da26')

build()
{
	Fbuild
	cd $Fdestdir/usr/bin
	for i in *.py
	do
		ln -s $i `basename $i .py`
	done
}
