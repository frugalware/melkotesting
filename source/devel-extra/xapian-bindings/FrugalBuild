pkgname=xapian-bindings
pkgver=1.2.8
pkgrel=1
pkgdesc="Python bindings for Xapian"
url="http://xapian.org/docs/bindings/python/"
depends=('python' "xapian-core=$pkgver")
groups=('devel-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://www.xapian.org/|grep 'stable version'|sed 's/.*is \(.*\), .*/\1/'"
source=(http://www.oligarchy.co.uk/xapian/$pkgver/$pkgname-$pkgver.tar.gz)
sha1sums=('92cf39c9c00a52a33727e47a1a7aaee2a1b23c2f')

# optimization OK
