pkgname=cvsps
pkgver=2.1
pkgrel=2
pkgdesc="Generates patchset information from a CVS repository"
url="http://www.cobite.com/cvsps/"
depends=('zlib')
groups=('devel-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url |grep 'recent stable version' |Flasttar"
source=($url/$pkgname-$pkgver.tar.gz)
sha1sums=('a53a62b121e7b86e07a393bcb8aa4f0492a747c4')


# optimization OK
