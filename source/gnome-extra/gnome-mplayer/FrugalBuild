pkgname=gnome-mplayer
pkgver=1.0.5
pkgrel=2
pkgdesc="A media player for GNOME that uses MPlayer."
archs=('i686' 'x86_64')
groups=('gnome-extra')
depends=('gmtk>=$pkgver' 'libnotify' 'libgpod' 'libuuid' \
	'libxscrnsaver' 'libmusicbrainz3')
Finclude googlecode
_F_gnome_glib="y"
_F_gnome_desktop="y"
_F_gnome_iconcache="y"
Finclude gnome-scriptlet
sha1sums=('780750f10b1b759bf638a34018ebb39bcd104bbb')

build ()
{
	Fbuild
	Fbuild_gnome_scriptlet
	Frm usr/share/doc/$pkgname
}

# optimization OK
