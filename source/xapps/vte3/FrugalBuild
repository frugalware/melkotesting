pkgname=vte3
_F_gnome_name=vte
pkgver=0.32.0
pkgrel=1
pkgdesc="Virtual Terminal Emulator (gtk+3)"
depends=('gtk+3')
rodepends=('vte-data')
makedepends=('gtk-doc>=1.11' 'perl-xml-parser' 'intltool' 'vala' 'gobject-introspection')
groups=('xapps')
archs=('i686' 'x86_64')
_F_gnome_ext=".tar.xz"
Finclude gnome
sha1sums=('2e22d77fd43dd0f758b8258f25aec82dc1c38232')
Fconfopts="$Fconfopts --enable-gtk-doc"

build() {
	Fbuild
	Frm usr/libexec/gnome-pty-helper
}

# optimization OK
