pkgname=libssh
pkgver=0.5.3
pkgrel=1
pkgdesc="The SSH library"
url="http://www.libssh.org/"
depends=('openssl>=1.0.0' 'zlib')
groups=('lib')
archs=('i686' 'x86_64' 'arm')
up2date="Flasttar $url/files/${pkgver%%.?}"
source=($url/files/${pkgver%%.?}/libssh-$pkgver.tar.gz)
sha1sums=('63f5f9e7d8b8e936048a51998a6f9be0e86866a7')
Finclude cmake
# optimization OK
